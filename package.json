{
  "name": "attr-styler",
  "description": "Make attributes for your styling",
  "version": "0.0.7",
  "type": "module",
  "main": "./build/attr-styler.umd.cjs",
  "module": "./build/attr-styler.js",
  "types": "./types/model.d.ts",
  "exports": {
    ".": {
      "types": "./types/model.d.ts",
      "require": "./build/attr-styler.umd.cjs",
      "import": "./build/attr-styler.js"
    }
  },
  "files": [
    "build",
    "types"
  ],
  "keywords": [
    "type",
    "attribute",
    "vite",
    "vite plugin",
    "vite-plugin"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc --project ./tsconfig.json --noEmit && eslint **/*.{ts,tsx,json} *.*",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/md5": "^2.3.5",
    "@types/node": "^22.15.1",
    "eslint": "^9.26.0",
    "react": "^19.1.1",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.1",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "md5": "^2.3.0",
    "typescript": "~5.7.2"
  }
}
